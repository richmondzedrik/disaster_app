{
    "version": 2,
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      },
      {
        "src": "backend/server.js",
        "src": "server.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server.js" 
      },
      {
        "handle": "filesystem"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/dist/index.html"
        "dest": "/server.js"
      }
    ]
  }